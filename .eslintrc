{
    "root": true,
    "settings": {
    },
    "plugins": ["@typescript-eslint"],
    "parser": "@typescript-eslint/parser",
    "extends": [
        "eslint:recommended", 
        "plugin:@typescript-eslint/recommended",
        "prettier",
    ],
    "rules": {
        "@typescript-eslint/explicit-module-boundary-types": 0,
        "@typescript-eslint/no-extra-semi": 0
    },
    "overrides": [
        {
            "files": ["**/*.ts?(x)"]
        }
    ]
}
